{
  "name": "musiy",
  "version": "1.0.0",
  "private": true,
  "description": "musiy project",
  "author": "Takaaki Shimbo",
  "scripts": {
    "build": "nuxt build",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "build-dev": "NODE_ENV=development nuxt build",
    "dev": "nuxt --max_old_space_size=2048 --optimize_for_size",
    "dev-debug": "node --inspect-brk=9229 node_modules/nuxt/bin/nuxt",
    "dev:sn": "SSL=true NO_LOGIN_STREAMING_TEST_MODE=true yarn dev",
    "fb_upload": "yarn generate && yarn fb_upload_hosting && firebase deploy --only firestore:rules && firebase deploy --only firestore:indexes && yarn fb_upload_functions",
    "fb_upload_dev": "NODE_ENV=development yarn generate && yarn fb_upload_hosting && firebase deploy --only firestore:rules && firebase deploy --only firestore:indexes && yarn fb_upload_functions",
    "fb_upload_functions": "firebase deploy --only functions",
    "fb_upload_functions_dev": "NODE_ENV=development firebase deploy --only functions",
    "fb_upload_hosting": "yarn generate && firebase deploy --only hosting",
    "fb_upload_hosting_dev": "NODE_ENV=development yarn generate && firebase deploy --only hosting",
    "generate": "nuxt generate",
    "lintfix": "eslint --fix --ext .js,.vue --ignore-path .gitignore .",
    "start": "nuxt start"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.32",
    "@fortawesome/free-brands-svg-icons": "^5.15.1",
    "@fortawesome/free-solid-svg-icons": "^5.15.1",
    "@fortawesome/vue-fontawesome": "^2.0.2",
    "@fracto/vue-credit-card": "^0.5.0",
    "@nuxtjs/axios": "^5.3.6",
    "@nuxtjs/dotenv": "^1.4.0",
    "@nuxtjs/pwa": "^3.0.0-0",
    "@stripe/stripe-js": "^1.9.0",
    "agora-access-token": "^2.0.4",
    "agora-rtc-sdk-ng": "^4.1.0",
    "animate.css": "^4.0.0",
    "axios": "^0.19.2",
    "cloudinary-vue": "^1.0.0-beta3",
    "core-js": "2.6.9",
    "firebase": "^7.23.0",
    "firebase-admin": "^9.11.0",
    "firebase-functions": "^3.14.1",
    "firebase-tools": "^8.3.0",
    "hls.js": "^0.14.17",
    "moment": "^2.26.0",
    "nuxt": "^2.0.0",
    "nuxt-user-agent": "^1.2.2",
    "screenfull": "^5.0.2",
    "skyway-js": "^4.0.0",
    "sora-js-sdk": "^2020.6.2",
    "ua-parser-js": "^0.7.22",
    "vue-cropperjs": "^4.2.0",
    "vue-prevent-unload": "^0.2.3",
    "vue-social-sharing": "^3.0.4",
    "vue-stripe-elements-plus": "^0.3.2",
    "vue-uuid": "^2.0.2",
    "vue2-touch-events": "^2.2.1",
    "vueinview": "^1.0.7",
    "vuetify-dialog": "^2.0.14",
    "vuex-persistedstate": "^3.0.1",
    "ysd-media-processor": "^1.1.4"
  },
  "devDependencies": {
    "@nuxtjs/eslint-config": "^2.0.0",
    "@nuxtjs/eslint-module": "^1.0.0",
    "@nuxtjs/vuetify": "^1.0.0",
    "@types/hls.js": "^0.13.3",
    "babel-eslint": "^10.0.1",
    "eslint": "^6.1.0",
    "eslint-plugin-nuxt": ">=0.4.2",
    "json-parse-even-better-errors": "^2.3.1",
    "vue-cli-plugin-cloudinary": "~0.0.5"
  }
